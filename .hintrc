{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 115",
    "not firefox <= 116",
    "not ios_saf <= 16.5",
    "not safari <= 16.5",
    "not and_chr <= 115",
    "not chrome <= 115",
    "not edge <= 115",
    "not opera <= 101",
    "not samsung <= 21"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ]
  }
}